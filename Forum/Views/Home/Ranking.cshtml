@{
    ViewBag.Title = "Ranking";
}

@model List<Forum.Models.ApplicationUser>

<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<table class="table">
    <tr>
        <th>
            UserName
        </th>
        <th>
            Point Count
        </th>
    </tr>
    @foreach (var user in ViewBag.Users)
    {
        <tr>
            <td>
                @user.UserName
            </td>
            <td>
                @user.PointCount
            </td>
        </tr>
    }
</table>
